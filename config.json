{
	"source": ["tokens/**/*.json"],
	"platforms": {
		"css": {
			"transformGroup": "css",
			"files": [
				{
					"destination": "css/variables.css",
					"format": "css/variables"
				}
			]
		},
		"scss": {
			"transformGroup": "scss",
			"files": [
				{
					"destination": "scss/_variables.scss",
					"format": "scss/variables"
				}
			]
		},
		"android": {
			"transformGroup": "android"
		}
	}
}
